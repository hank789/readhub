{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/voten-co/laravel-scout-elastic"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Tlapi/slack-laravel.git"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "embed/embed": "3.*",
        "intervention/image": "^2.3",
        "flugger/laravel-responder": "^1.1.2",
        "pusher/pusher-php-server": "^2.6",
        "laravel/scout": "3.*",
        "zachleigh/laravel-vue-generators": "^0.1.1",
        "guzzlehttp/guzzle": "^6.2",
        "jacobcyl/ali-oss-storage": "dev-master",
        "symfony/filesystem": "^3.3",
        "predis/predis": "^1.1",
        "pbmedia/laravel-ffmpeg": "^1.1",
        "laravel/socialite": "^3.0",
        "sentry/sentry": "^1.6",
        "sentry/sentry-laravel": "^0.6.1",
        "jacobbennett/laravel-http2serverpush": "^1.3",
        "laravel/passport": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.5",
        "laravel/tinker": "^1.0",
        "overtrue/laravel-pinyin": "~3.0",
        "tlapi/slack-laravel": "dev-master",
        "whichbrowser/parser": "^2.0",
        "yab/laravel-scout-mysql-driver": "^2.0",
        "publiux/laravelcdn": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laravel/dusk": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Http/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
