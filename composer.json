{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/voten-co/laravel-scout-elastic"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Tlapi/slack-laravel.git"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.5",
        "embed/embed": "3.*",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "jacobbennett/laravel-http2serverpush": "^1.3",
        "laravel/framework": "5.5.*",
        "laravel/horizon": "^1.0",
        "laravel/passport": "^3.0",
        "laravel/scout": "3.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "pbmedia/laravel-ffmpeg": "^1.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^2.6",
        "sentry/sentry": "^1.6",
        "sentry/sentry-laravel": "^0.6.1",
        "whichbrowser/parser": "^2.0",
        "zachleigh/laravel-vue-generators": "^0.1.1",

        "jacobcyl/ali-oss-storage": "dev-master",
        "symfony/filesystem": "^3.3",
        "overtrue/laravel-pinyin": "~3.0",
        "tlapi/slack-laravel": "dev-master",
        "publiux/laravelcdn": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
      "classmap": [
        "database/seeds",
        "database/factories"
      ],
      "files": [
        "app/Http/helpers.php"
      ],
      "psr-4": {
        "App\\": "app/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      },
      "files": ["tests/utilities/functions.php"]
    },
    "extra": {
      "laravel": {
        "dont-discover": [
        ]
      }
    },
    "scripts": {
        "post-root-package-install": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
          "@php artisan key:generate"
        ],
        "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate",
          "@php artisan ide-helper:generate",
          "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
          "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
